package casting;

public class Mainmath {
	
	public static void main(String[] args) {
		
		double x = 3.0;
		double y = 4.0;
		double z = -5.0;
		
		double a, b, c;
		
		a = Math.sqrt(x);
		b = Math.sqrt(y);
		c = Math.sqrt(25.00);
		System.out.printf("Raiz quadra de %f é %.2f%n", x, a);
		System.out.printf("Raiz quadra de %f é %.2f%n", y, b);
		System.out.printf("Raiz quadra de 25 é %.2f%n", c);
		
		
		
		
		
		

	}

}
